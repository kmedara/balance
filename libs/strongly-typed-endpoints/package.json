{
  "name": "strongly-typed-endpoints",
  "version": "1.0.0",
  "description": "enforce schema at the type level for endpoint definitions.",
  "main": "dist/index.js",       
  "module": "dist/index.mjs",   
  "types": "dist/index.d.ts",   
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
   "build": "tsup src/index.ts --format cjs,esm --dts",
    "prepare": "npm run build"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "devDependencies": {
    "@types/node": "^25.2.0",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3"
  }
}
